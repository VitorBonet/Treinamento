*/
Function NGCHKTRVER(cvArqP,cvArqC,cvTipC,nDias)

	Local oTmpTbl
	Local cTRB := GetNextAlias()
	Private cArqTXT1 := cvArqP,cArqTXT2 := cvArqC,cTipoTXT := cvTipC,aVETINR := {}
	Private lDiasAt  := If(nDias = Nil,.F.,.T.), nDiasAt := If(nDias = Nil,0,nDias)
	Private cVersRPO := Alltrim(cVersao)
	cVersRPO         := STRTRAN(cVersRPO,".","")

	aDBF := {{"PROGRAMA","C",15,0},;
	{"DATANG"  ,"D",08,0},;
	{"DATACL"  ,"D",08,0},;
	{"PARECE"  ,"C",50,0}}

	vIND := {"PROGRAMA"}

	// Cria arquivos temporarios
	oTempTable := FWTemporaryTable():New( cTRB, aDBF )
	oTempTable:AddIndex( "Ind01" , vIND )
	oTempTable:Create()

	Processa({ |lEnd| NGCOMTXTV(lDiasAt,nDiasAt) },STR0228) //"Aguarde... filtrando e gerando arquivos."

	NGSETIFARQUI(cTRB)
	If Reccount() > 0
		NGVEBROW()
		DbselectArea(cTRB)
		Copy To NGDIFVER
		cEndArq := AllTrim(GetSrvProfString("RootPath"," "));
		+AllTrim(GetSrvProfString("StartPath"," "))
		cEndArq := If (isSRVunix(),StrTran(cEndArq,"//","/"),StrTran(cEndArq,"\\","\"))
		MsgInfo(STR0229+" : "+cEndArq+" NGDIFVER.DBF",STR0025) //"Arquivo para consulta disponivel em"###"ATENCAO"
	Else
		MsgInfo(STR0230,STR0025) //"Nao ha problema na versao dos fontes"###"ATENCAO"
	Endif

	// deleta arquivos temporarios
	oTmpTbl:Delete()

	DbselectArea("SX3")
Return .T.


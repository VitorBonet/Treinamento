Function NgHelp(cHelp,cHlpVar,lSX1,cLang,cSolucao)
	Local nRet, nI, nT
	Local lNew := .F.
	Local cNewMemo, cLast, cFile

	Default lSX1     := .F.
	Default cSolucao := ""

	If Empty(cHelp)
		Return
	Else
		cHelp := AllTrim(Upper(cHelp))
	EndIf

	If cLang == Nil
		#IFDEF SPANISH
		cLang := "S"
		#ELSE
		#IFDEF ENGLISH
		cLang := "E"
		#ELSE
		cLang := "P"
		#ENDIF
		#ENDIF
	EndIf

	//Arquivo Help da linguagem
	cFile := "SIGAHL"+cLang+".HL"+cLang //SIGAHLP.HLP - SIGAHLE.HLE - SIGAHLS.HLS
	//Portugues   - English     - Spanish

	//Conteudo do "Problema" do Help
	If !Empty(cHlpVar)

		cHelpNew := If(lSX1,SubStr(cHelp,1,7)+SubStr(cHelp,12,3),cHelp)
		nRet     := SPF_SEEK(cFile,"P"+cHelpNew,1)

		If nRet > 0
			SPF_UPDATE(cFile,nRet,"P"+cHelpNew,"MODIFIED",,cHlpVar)
		Else
			SPF_INSERT(cFile,"P"+cHelpNew,"USER",,cHlpVar)
			lNew := .T.
		EndIf
	EndIf

	//Conteudo da "Solucao" do Help
	If !Empty(cSolucao)

		cHelpNew := If(lSX1,SubStr(cHelp,1,7)+SubStr(cHeladminp,12,3),cHelp)
		nRet     := SPF_SEEK(cFile,"S"+cHelpNew,1)

		If nRet > 0
			SPF_UPDATE(cFile,nRet,"S"+cHelpNew,"MODIFIED",,cSolucao)
		Else
			SPF_INSERT(cFile,"S"+cHelpNew,"USER",,cSolucao)
			lNew := .T.
		EndIf
	EndIf

	If lSX1
		DbSelectArea("SX1")
		DbSetorder(01)
		If Dbseek(SubStr(cHelp,2,12))
			RecLock("SX1",.F.)
			SX1->X1_HELP := SubStr(cHelp,1,7)+SubStr(cHelp,12,3)
			MsUnLock("SX1")
		Endif
	EndIf

Return


Function NGCAMPNSX3(cVALIAS,vVNCAMPO,lBROWSE, lUser, lTodo)
	Local vCAMPOSX3 := {}
	Local aArea     := GetArea()

	Default cVALIAS  := Alias()
	Default vVNCAMPO := {}
	Default lBROWSE  := .F.
	Default lUser    := .T.
	Default lTodo    := .F.

	DbSelectArea("SX3")
	DbSetOrder(1)
	If DbSeek(cVALIAS)
		While !Eof() .And. (sx3->x3_arquivo == cVALIAS)
			If ( X3USO(sx3->x3_usado) .And. cNivel >= sx3->x3_nivel ) .Or. lTodo
				If Ascan(vVNCAMPO,{|x| Alltrim(Upper(x)) == Alltrim(sx3->x3_campo)}) = 0
					If lBROWSE .And. SX3->X3_BROWSE != 'S'
						dbSkip()
						Loop
					EndIf
					If !lUser .And. AllTrim(SX3->X3_PROPRI) == "U"
						dbSkip()
						Loop
					EndIf
					Aadd(vCAMPOSX3,sx3->x3_campo)
				Endif
			Endif
			DbSkip()
		End
	Endif

	RestArea(aArea)

Return vCAMPOSX3


Function NGPRODESP(cCodPro,lSaida,cParam)
	Local lSaiTel := If(lSaida = Nil,.T.,lSaida),vVetRet := {.T.,"  "}
	Local cVerPar := If(cParam == Nil, "*", cParam), aProdutos := {}
	Local cCodTer := ""
	Local cCodMan := ""
	Local lRet    := .T.

	If !NGIFDBSEEK("SB1",AllTrim(cCodPro),1,.F.)
		vVetRet := {.F.,"RECNOIS"}
	ElseIf nModulo = 19 // Sigamnt
		If !(AllTrim(GetNewPar("MV_NGINTER","N")) == "L")
			//Se o Produto for de Controle de Manutencao, retorna 'Falso'
			If SubStr(cCodPro,1,3) == "MOD"
				vVetRet:= {.F.,"NGMMODTEMA"}
				Return vVetRet[1]
			EndIf
		EndIf
		If FindFunction("NGProdMNT")
			aProdutos := aClone(NGProdMNT(cVerPar))
			If aScan(aProdutos, {|x| AllTrim(x) == AllTrim(cCodPro) }) > 0
				vVetRet:= {.F.,"NGMMODTEMA"}
			EndIf
		Else
			cCodTer := GetMv("MV_PRODTER")
			cCodMan := GetMv("MV_PRODMNT")

			lRet := .T.
			If cVerPar == "*" .And. ( cCodPro = SubStr(cCodTer,1,Len(SB1->B1_COD)) .Or. cCodPro = SubStr(cCodMan,1,Len(SB1->B1_COD)) )
				lRet := .F.
			ElseIf cVerPar == "T" .And. ( cCodPro = SubStr(cCodTer,1,Len(SB1->B1_COD)) )
				lRet := .F.
			ElseIf cVerPar == "M" .And. ( cCodPro = SubStr(cCodMan,1,Len(SB1->B1_COD)) )
				lRet := .F.
			EndIf

			If !lRet
				vVetRet:= {.F.,"NGMMODTEMA"}
			EndIf
		EndIf
	ElseIf nModulo = 35 // Sigamdt
		// Incluir as validacoes

	ElseIf nModulo = 56 // Sigasga
		// Incluir as validacoes

	EndIf

	If !vVetRet[1] .And. lSaiTel
		Help(" ",1,vVetRet[2] )
	EndIf

Return vVetRet[1]


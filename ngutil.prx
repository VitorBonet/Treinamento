/*/{Protheus.doc} NGAtuErp
Faz a geração dos registros para a tabela de integração com outros ERP's.

@author Rafael Diogo Richter
@since 07/05/2008

@param cAlias - Indica o código do produto
@param cTipo - Indica o código do almoxarifado

@return .T.
/*/
//---------------------------------------------------------------------
Function NGAtuErp(cAlias,cOper)

	Local aAreaErp := GetArea()

	Local g, t, y, x

	Local aInt		 := {}
	Local aSet		 := {}
	Local aTabela	 := {}
	Local aCampAtu := {}

	Local __nConecta

	If ExistBlock("NGAtuErp")
		ExecBlock("NGAtuErp", .F., .F., { cAlias, cOper } )
		Return
	EndIf

	If AllTrim( GetNewPar("MV_NGINTER", "N") ) == "L"

		If !FindFunction("NgIntLogix")
			MsgStop("Função NgIntLogix não encontrada!")
			Return .F.
		EndIf

		//Tabelas não serão enviadas ao Logix
		If cAlias == "SB2" .Or. cAlias == "SC1" .Or. cAlias == "SC2" .Or. cAlias == "SB1"
			Return .F.
		EndIf

		//Tratamento para devolução (DE1) enviar operação DELETE
		If cAlias == "SD3" .And. SD3->D3_CF == 'DE1'
			cOper := 'DELETE'
		EndIf

		t := 1

		If cOper == "INSERT"

			aAdd( aInt, {"INSERT", "tabela", cAlias, 3, t, 1, 1 } )

		ElseIf cOper == "UPDATE"

			c := 1
			aAdd( aInt, {"UPDATE", "tabela", cAlias, 3, t, 1, 1} )

		ElseIf cOper == "DELETE"

			aAdd( aInt, {"DELETE", "tabela", cAlias, 3, t, 1, 1} )

		EndIf

		For g := 1 To (cAlias)->( FCount() )

			cCampo		:= ""
			nTipCamp	:= 0

			If ValType( &(cAlias + "->" + (cAlias)->(FieldName(g))) ) == "C"

				cCampo := &(cAlias + "->" + (cAlias)->(FieldName(g)) )
				nTipCamp := 3

			ElseIf ValType( &(cAlias+"->"+(cAlias)->(FieldName(g))) ) == "N"

				cCampo := Alltrim(Str(&(cAlias+"->"+(cAlias)->(FieldName(g)))))
				nTipCamp := 5

			ElseIf ValType( &(cAlias+"->"+(cAlias)->(FieldName(g))) ) == "D"

				cCampo := DToC( &(cAlias+"->"+(cAlias)->(FieldName(g))) )
				nTipCamp := 4

			EndIf

			If cOper == "INSERT"

				aAdd( aInt, {"INSERT", (cAlias)->(FieldName(g)),	cCampo, nTipCamp, t, 1, g + 1} )

			ElseIf cOper == "UPDATE"

				dbSelectArea("SIX")
				dbSetOrder(1)
				dbSeek(cAlias)
				If (cAlias)->( FieldName(g) ) $ SIX->CHAVE

					c++
					aAdd( aInt, {"WHERE", (cAlias)->(FieldName(g)), cCampo, nTipCamp, t, 1, c} )
					Loop

				EndIf

				aAdd( aSET, {"SET", (cAlias)->(FieldName(g)), cCampo, nTipCamp, t, 1, 2} )

			ElseIf cOper == "DELETE"

				dbSelectArea("SIX")
				dbSetOrder(1)
				dbSeek(cAlias)
				If ((cAlias)->(FieldName(g)) $ SIX->CHAVE) .Or. ;
				((cAlias)->(FieldName(g)) == 'D3_LOTECTL' .Or.;
				(cAlias)->(FieldName(g)) == 'D3_NUMLOTE')

					aAdd( aInt, {"WHERE",(cAlias)->(FieldName(g)),	cCampo	,nTipCamp,t,1,g+1} )

				EndIf
			EndIf

		Next g

		If cOper == "INSERT"

			U_MNTFields(@aInt, cAlias)

		ElseIf cOper == "UPDATE"

			For y := 1 To Len(aSET)
				c++
				aAdd( aInt, {aSet[y][1], aSet[y][2], aSet[y][3], aSet[y][4], aSet[y][5], aSet[y][6], c} )
			Next y

		EndIf

		//Envia o usuário logado que realizou a inclusão\alteração\exclusão
		If cOper == "INSERT"
			aAdd( aInt, {"INSERT","USUARIO",cUserName	,3,t,1,g+2} )
		ElseIf cOper == "UPDATE"
			aAdd( aInt, {"SET","USUARIO",cUserName	,3,t,1,g+2} )
		EndIf

		NGIntLogix("S",aInt)

	ElseIf AllTrim( GetNewPar("MV_NGINTER","N") ) == "R"

		cAliasBD := AllTrim( GetNewPar("MV_NGINTDB","") )

		If Empty(cAliasBD)
			ShowHelpDlg( "MV_NGINTDB", { "O parâmetro MV_NGINTDB não foi configurado." }, 5,;
			{"Configure o parâmetro para prosseguir."}, 5)
			Return .F.
		EndIf

		If cOper == "INSERT"
			cOper := "I"
		ElseIf cOper == "UPDATE"
			cOper := "A"
		ElseIf cOper == "DELETE"
			cOper := "E"
		EndIf

		dbSelectArea("TTU")
		dbSetOrder(1)
		If dbSeek( xFilial("TTU") + "2" + "1" + cAlias )

			cAliasRM := TTU->TTU_TABRM
			aCampAtu := {}

			While !EoF() .And. TTU->TTU_FILIAL == xFilial("TTU") .And. TTU->TTU_TPSINC == "2" .And.;
			TTU->TTU_TIPREL == "1" .And. AllTrim(TTU->TTU_TABMIC) == cAlias

				lEnd := .F.
				lTabDif := .F.

				dbSkip()

				If EoF() .Or. AllTrim(TTU->TTU_TABMIC) <> cAlias
					lEnd := .T.

					If AllTrim(TTU->TTU_TABMIC) <> cAlias
						lTabDif := .T.
					EndIf
				EndIf

				dbSkip(-1)

				If cAliasRM <> TTU->TTU_TABRM .Or. lEnd

					If lEnd
						aAdd(aCampAtu, {TTU->TTU_TABMIC, TTU->TTU_TABRM, TTU->TTU_CAMPMI, TTU->TTU_CAMPRM})
					EndIf

					cAliasQry := GetNextAlias()

					cQuery := "SELECT COUNT(*) nQtd "
					cQuery += "FROM " + aCampAtu[1][2]
					cQuery += " WHERE "

					For x := 1 To Len(aCampAtu)

						cQuery += AllTrim(aCampAtu[x][4])+" = "

						If ValType( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) == "C"

							If AllTrim(aCampAtu[x][4]) == "IDPRD" .Or.;
							AllTrim(aCampAtu[x][4]) == "NSEQITMMOV" .Or.;
							AllTrim(aCampAtu[x][4]) == "IDMOV"

								cQuery += &(aCampAtu[x][1] + "->" + aCampAtu[x][3])
							Else
								cQuery += "'" + &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) + "'"
							EndIf

						ElseIf ValType( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) == "N"

							cQuery += Alltrim( Str( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) )

						ElseIf ValType( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) == "D"

							cQuery += "'" + DToS( &(aCampAtu[x][1] + "->" + aCampAtu[x][3])) + "'"

						Else
							cQuery += "' '"
						EndIf

						If x < Len(aCampAtu)
							cQuery += " And "
						EndIf
					Next x

					aAdd(aTabela, { aCampAtu[1][2], "ERP" })

					__nConecta := getConnect(aTabela, cAliasBD)
					TcSetConn(__nConecta)

					cQuery := ChangeQuery(cQuery)
					dbUseArea( .T., "TOPCONN", TCGENQRY(,,cQuery), cAliasQry, .F., .T.)

					dbSelectArea(cAliasQry)
					If (cAliasQry)->nQtd > 0

						(cAliasQry)->(dbCloseArea())

						TcUnlink(__nConecta)
						TcSetConn( AdvConnection() )

						aTabela	:= {}
						aCampAtu	:= {}
						cAliasRM	:= TTU->TTU_TABRM

						If !lTabDif
							dbSelectArea("TTU")
							dbSetOrder(1)
							dbSkip(-1)
						EndIf

						dbSelectArea("TTU")
						dbSetOrder(1)
						dbSkip()
						Loop
					EndIf

					TcUnlink(__nConecta)
					TcSetConn( AdvConnection() )

					cQuery := "INSERT INTO " + cAliasRM + " ("

					For x := 1 To Len(aCampAtu)
						cQuery += aCampAtu[x][4] + ", "
					Next x

					aAreaTTU := TTU->(GetArea())

					dbSelectArea("TTU")
					dbSetOrder(2)
					dbSeek( xFilial("TTU") + "2" + "3" + cAliasRM )
					While !EoF() .And. TTU->TTU_FILIAL == xFilial("TTU") .And.;
					TTU->TTU_TPSINC == "2" .And. TTU->TTU_TIPREL == "3" .And.;
					TTU->TTU_TABRM == cAliasRM

						cQuery += TTU->TTU_CAMPRM + ", "

						dbSelectArea("TTU")
						dbSetOrder(2)
						dbSkip()
					EndDo

					RestArea(aAreaTTU)

					cQuery += "STATUS_INT, OPERACAO_INT, DATAINSERCAO_INT) "
					cQuery += "VALUES ("

					For x := 1 To Len(aCampAtu)

						If ValType( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) == "C"

							If AllTrim(aCampAtu[x][4]) == "IDPRD" .Or.;
							AllTrim(aCampAtu[x][4]) == "NSEQITMMOV" .Or.;
							AllTrim(aCampAtu[x][4]) == "IDMOV"

								cCampMic := &(aCampAtu[x][1] + "->" + aCampAtu[x][3])
							Else
								cCampMic := "'" + &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) + "'"
							EndIf

						ElseIf ValType( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) == "N"

							cCampMic := AllTrim( Str( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) )

						ElseIf ValType( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) == "D"

							cCampMic := "'" + DToS( &(aCampAtu[x][1] + "->" + aCampAtu[x][3]) ) + "'"

						Else
							cCampMic := "' '"
						EndIf

						cQuery += cCampMic + ", "

					Next x

					aAreaTTU := TTU->(GetArea())

					dbSelectArea("TTU")
					dbSetOrder(2)
					dbSeek( xFilial("TTU") + "2" + "3" + cAliasRM )
					While !Eof() .And. TTU->TTU_FILIAL == xFilial("TTU") .And.;
					TTU->TTU_TPSINC == "2" .And. TTU->TTU_TIPREL == "3" .And.;
					TTU->TTU_TABRM == cAliasRM

						cQuery += TTU->TTU_VALDEF + ", "

						dbSelectArea("TTU")
						dbSetOrder(2)
						dbSkip()
					EndDo

					RestArea(aAreaTTU)

					cQuery += "'I', '" + cOper + "', '" + DToS(dDatabase) + "')"

					aAdd(aTabela,{cAliasRM,"ERP"})
					__nConecta := getConnect(aTabela,cAliasBD)
					TcSetConn(__nConecta)

					If TcSqlExec(cQuery) <> 0
						UserException(TcSQLError())
					EndIf

					TcUnlink(__nConecta)
					TcSetConn(AdvConnection())

					aTabela	:= {}
					aCampAtu	:= {}
					cAliasRM	:= TTU->TTU_TABRM

					If !lTabDif
						dbSelectArea("TTU")
						dbSetOrder(1)
						dbSkip(-1)
					EndIf
				Else
					aAdd(aCampAtu,{TTU->TTU_TABMIC,TTU->TTU_TABRM,TTU->TTU_CAMPMI,TTU->TTU_CAMPRM})
				EndIf

				dbSelectArea("TTU")
				dbSetOrder(1)
				dbSkip()
			EndDo
		EndIf

	ElseIf AllTrim( GetNewPar("MV_NGINTER","N") ) == "M"  //Mensagem Unica

		//Solicitação de Compras
		If cAlias == 'SC1'
			If cOper == "DELETE"
				NGMUReques(SC1->(RecNo()),"SC1",.F.,5)
			EndIf
		EndIf

	EndIf

	RestArea(aAreaErp)

Return .T.


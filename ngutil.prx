/*/
FUNCTION MTOH(nMINUTO)
	Local nHoras,nRest,cRet,cHoraC := Alltrim(Str(nMINUTO))
	Local nPonVi := At(".",cHoraC)
	Local lNegativo := .F.
	If nPonVi = 0
		nPonVi := At(",",cHoraC)
	Endif

	nHoras := INT(nMINUTO / 60)
	nRest  := INT(nMINUTO % 60)

	If nPonVi = 0
		nPonVi := At(",",cHoraC)
	Endif
	If nPonVi > 0
		nMinuM := Val(Substr(cHoraC,nPonVi+1,1))
		If nMinuM > 5
			nRest += 1
		Endif
	Endif

	If nMINUTO < 0
		lNegativo := .T.
		nHORAS := nHORAS * -1
	Endif
	If nRest < 0
		nRest := nRest * -1
	Endif

	cRet   := StrZero(nHoras,2) + ":" + StrZero(nRest,2)

	If nHORAS > 99
		cRET := StrZero(nHoras,3) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 999
		cRet := StrZero(nHoras,4) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 9999
		cRet := StrZero(nHoras,5) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 99999
		cRet := StrZero(nHoras,6) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 999999
		cRet := StrZero(nHoras,7) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 9999999
		cRet := StrZero(nHoras,8) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 99999999
		cRet := StrZero(nHoras,9) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 999999999
		cRet := StrZero(nHoras,10) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 9999999999
		cRet := StrZero(nHoras,11) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 99999999999
		cRet := StrZero(nHoras,12) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 999999999999
		cRet := StrZero(nHoras,13) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 9999999999999
		cRet := StrZero(nHoras,14) + ":" + StrZero(nRest,2)
	Endif
	If nHORAS > 99999999999999
		cRet := StrZero(nHoras,15) + ":" + StrZero(nRest,2)
	Endif

	If lNegativo .And. At('-',cRET) == 0
		cRET := '-'+cRET
	Endif

Return cRET


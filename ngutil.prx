Function NGCHKVER(cArqPrim,cArqComp,cTipComp)
	Local nOpcax := 0,oDlga
	Private cArvTXT1 := cArqPrim, cArqTXT1 := cArvTXT1
	Private cArvTXT2 := cArqComp, cArqTXT2 := cArvTXT2

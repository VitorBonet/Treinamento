/*/
Function NGSALSB2(cVCOD,cALMOX,nQUANT,lMENSA,nDevol,dData,lConAlt)
	Local cALIASOLD := ALIAS()
	Local nOLDKEY   := INDEXORD()
	Local cALMOXAF  := If(Empty(cALMOX),NGSEEK("SB1",cVCOD,1,"B1_LOCPAD"),cALMOX)
	Local lPROBLEM  := .T.
	Local   lSAIDAME	:= If(lMENSA = Nil,.T.,lMENSA)
	Local nDevEst   := If(nDevol = Nil,0,nDevol)
	Local	 cTpSaldo		:= AllTrim(SuperGetMV("MV_TPSALDO"))
	Default dData			:= dDataBase
	Default lConAlt := INCLUI //Verificar se valida alteração

	DbSelectArea("SB2")
	DbSetOrder(1)
	If !Dbseek(xFilial('SB2') + cVCOD + cALMOXAF)
		CriaSB2(cVCOD,cALMOXAF)
	EndIf

	If lPROBLEM
		If GetNewPar("MV_NGINTER","") == "M" 			// Integracao por Mensagem Unica
			nSALDOP := NGMUStoLvl(cVCOD, cALMOXAF,.F.) // Atualiza tabela
		Else
			If cTpSaldo == "C" //Busca saldo que o estoque tinha na data informada no parâmetro dData
				nSALDOP := CalcEst(SB2->B2_COD,SB2->B2_LOCAL,dData+1)[1]
			ElseIf cTpSaldo == "S" //Retorna saldo atual independente da data
				nSALDOP := SaldoSB2(.F.,.F.,dDataBase+3650,.F.)+nDevEst
			ElseIf cTpSaldo == "Q" //Retorna saldo com desconto de quantidade reservada e quantidade a enderecar
				nSALDOP := SB2->B2_QATU - SB2->B2_QACLASS - SB2->B2_RESERVA
			EndIf
		EndIf
		If (INCLUI .Or. lConAlt) .And. nSALDOP < nQUANT
			If lSAIDAME
				MsgInfo(STR0270+Chr(13)+Chr(13); //"Foi Especificado No Parametro De Controle do Estoque"
				+STR0271+chr(13)+Chr(13); //"Que o Mesmo Nao Podera Ficar Negativo."
				+STR0272+AllTrim(cVCOD)+"   "+STR0273+cALMOXAF+Chr(13)+Chr(13); //"Produto      -> "###"Deposito      -> "
				+STR0274+Str(nSALDOP,14,2)+Chr(13)+Chr(13); //"Saldo Atual -> "
				+STR0275+Str(nQUANT,14,2),STR0223) //"Quant. Item -> "###"com sucesso"
			Endif
			lPROBLEM := .F.
		EndIf
	EndIf

	DbSelectArea(cALIASOLD)
	DbSetOrder(nOLDKEY)
Return lPROBLEM


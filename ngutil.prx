/*/{Protheus.doc} NGCRIATRB
Criação de arquivos temporários

@author Inacio Luiz Kolling
@param aField  - Matriz com as definicoes dos campos	-Nao Obrig.
@param aIndex  - Vetor com as definicoes dos indices	-Obrigatorio
@param cTrb    - Alias do arquivo temporario         	-Obrigatorio
@param cAlias  - Alias do arquivo replica            	-Nao Obrig.
@param cDrive  - Formato do drive utilizado            -Nao Obrig.
@since 04/10/2005
@return cArqTrb Nome do arquivo gerado pela CriaTrab

@Obs Uns dos parametros (aField ou cAlias) sera Obrigatorio
/*/
//---------------------------------------------------------------------
Function NGCRIATRB(aField,aIndex,cTrb,cAlias,cDrive)
	Local nX, nIV
	Local aStruct
	Local cArqTrb

	Default cDrive:= nil

	If ValType(cAlias) == "C"
		dbSelectArea(cAlias)
		aStruct := dbStruct()

		If ValType(aField) == "A" .And. Len(aField) > 0
			For nX := 1 To Len(aField)
				aAdd(aStruct,aField[nX])
			Next nX
		Endif

		aField := aStruct
	Endif

	cArqTrb := CriaTrab(aField)

	dbUseArea(.T.,cDrive,cArqTrb,cTrb,.F.,.F.)

	aAdd(aTrbIndex,{})
	nIV := Len(aTrbIndex)

	For nX := 1 To Len(aIndex)
		cINDX := "cID"+cTrb+ cValToChar(nX)
		Private &(cINDX) := Left(cArqTrb,7)+ cValToChar(nX)
		aAdd(aTrbIndex[nIV],{cTrb,&(cINDX)})
		IndRegua(cTrb,aTrbIndex[nIV,nX,2],aIndex[nX],,,STR0111)
	Next nIC

	dbClearIndex()
	For nX := 1 To Len(aTrbIndex[nIV])
		DbSetIndex(aTrbIndex[nIV,nX,2] + OrdBagExt())
	Next nX

Return cArqTrb

//---------------------------------------------------------------------
/*/{Protheus.doc} NGDELETRB
Delecao de arquivos temporarios

@author Inacio Luiz Kolling
@param cTrb  - Alias do arquivo temporario         -Obrigatorio
@param cArqTrb  - Nome do arquivo temporario          -Obrigatorio
@since 04/10/2005
@return cArq Nome do arquivo gerado pela CriaTrab

/*/
//---------------------------------------------------------------------
Function NGDELETRB(cTrb,cArqTrb)

	Local nY, nX

	dbSelectArea(cTrb)
	Use

	If ValType(cArqTrb) == "C"
		FErase(cArqTrb  + GetDbExtension())
		If File(cArqTrb+".fpt")
			FErase(cArqTrb+".fpt")
		Endif
	EndIf

	For nY := Len(aTrbIndex) To 1 Step -1
		For nX := Len(aTrbIndex[nY]) To 1 Step -1
			If aTrbIndex[nY][nX][1] == cTrb
				FErase(aTrbIndex[nY,nX,2] + OrdBagExt())
				aDel(aTrbIndex[nY], nX)
				aSize(aTrbIndex[nY], nX-1)
			EndIf
		Next nX

		If Len(aTrbIndex[nY]) == 0
			aDel(aTrbIndex,nY)
			aSize(aTrbIndex, Len(aTrbIndex)-1)
		EndIf
	Next nY

Function NGPROXSX9(cDominio)
	Local cIdent := "000"
	Local aArea  := GetArea()
	If cDominio = Nil
		Return "001"
	ElseIf Empty(cDominio)
		Return "001"
	Endif

	dbSelectArea("SX9")
	dbSetOrder(1)
	dbSeek(cDominio)
	While !Eof() .and. AllTrim(SX9->X9_DOM) == cDominio
		If Val(SX9->X9_IDENT) > Val(cIdent)
			cIdent := SX9->X9_IDENT
		Endif
		dbSelectArea("SX9")
		dbSetOrder(1)
		dbSkip()
	End

	If FindFunction("Soma1Old")
		cIdent := Soma1Old(cIdent)
	Else
		cIdent := Soma1(cIdent)
	Endif
	RestArea(aArea)
Return cIdent


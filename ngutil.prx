Function NGCADICBASE(cCampP,cTipoP,cAlias,lTelaS,cEmpresa)
	Local aAreaTemF := GetArea(), vRetPesq  := {.T.,Space(1)}

	//Abre dicionário de outra empresa
	If !Empty(cEmpresa)
		NGABRESX("SIX",cEmpresa)
		NGABRESX("SX2",cEmpresa)
		NGABRESX("SX3",cEmpresa)
	EndIf

	If cTipoP $ "DA"
		If Empty(NGSEEKDIC("SX2",cAlias,1,"X2_NOME"))
			vRetPesq := {.F.,STR0149+" "+cAlias+" "+STR0148+" (SX2)"}
		ElseIf Empty(NGSEEKDIC("SIX",cAlias,1,"INDICE"))
			vRetPesq := {.F.,STR0149+" "+cAlias+" "+STR0148+" (SIX)"}
		ElseIf Empty(NGSEEKDIC("SX3",cCampP,2,"X3_TITULO"))
			vRetPesq := {.F.,STR0147+" "+cCampP+" "+STR0148+" (SX3)"}
		Endif
	Endif

	If vRetPesq[1] .And. cTipoP $ "BA"
		If Empty(NGSEEKDIC("SX2",cAlias,1,"X2_NOME"))
			vRetPesq := {.F.,STR0149+" "+cAlias+" "+STR0148+" (SX2)"}
		Endif
		If vRetPesq[1]
			dbSelectArea(cAlias)
			If FieldPos(cCampP) = 0
				vRetPesq := {.F.,STR0147+" "+cCampP+" "+STR0150+" ("+cAlias+")"}
			Endif
		Endif
	Endif

	//Volta dicionario da empresa corrente
	If !Empty(cEmpresa)
		NGABRESX("SIX",cEmpAnt)
		NGABRESX("SX2",cEmpAnt)
		NGABRESX("SX3",cEmpAnt)
	EndIf

	If lTelaS <> Nil
		If lTelaS
			If !vRetPesq[1]
				MsgInfo(vRetPesq[2],STR0053)
			Endif
		Endif
		RestArea(aAreaTemF)
		Return vRetPesq[1]
	Endif

	RestArea(aAreaTemF)
Return vRetPesq


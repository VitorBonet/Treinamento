Function NGHEXRGB(cClrHex)
	Local aRGB := Array(3)

	Default cClrHex := "000000"

	If Len(cClrHex) == 3
		cClrHex += cClrHex
	EndIf

	aRGB[1] := CTON(Substr(cClrHex,1,2),16)
	aRGB[2] := CTON(Substr(cClrHex,3,2),16)
	aRGB[3] := CTON(Substr(cClrHex,5,2),16)


	cVersRPO         := STRTRAN(cVersRPO,".","")

	@ 200,190 TO 420,615 DIALOG oDLGA TITLE STR0025 //"ATENCAO"
	@ 012,3 TO 105,210

	@ 20,015 say OemtoAnsi(STR0224+" "+STR0225) //"Este programa tem por objetivo"###"MOSTRAR / LISTAR as diferencas entre"
	@ 28,007 say OemtoAnsi(STR0226+" "+cVersRPO) //"dois arquivos em TXT, contendo os fontes da versao"

Function NGCRIASXB(vAlias,aSXBI)

	//+-----------------------------------------------------------------------+
	//| não deve ser alterado dicionário via aplicação, apos garantir que tal |
	//| prática não ocorre mais, deverá ser removida esta função              |
	//+-----------------------------------------------------------------------+

	Local aAreaXB := GetArea()
	Local nFl := 0
	DbSelectArea("SXB")
	DbSetOrder(1)
	If !Dbseek(vAlias)
		For nFl := 1 To Len(aSXBI)
			RecLock("SXB",.T.)
			SXB->XB_ALIAS := vAlias
			SXB->XB_TIPO  := aSXBI[nFl,1]
			SXB->XB_SEQ   := aSXBI[nFl,2]
			If Len(aSXBI[nFl]) > 2 .And. aSXBI[nFl,3] <> Nil
				SXB->XB_COLUNA := aSXBI[nFl,3]
			Endif
			If Len(aSXBI[nFl]) > 3 .And. aSXBI[nFl,4] <> Nil
				SXB->XB_DESCRI := aSXBI[nFl,4]
			Endif
			If Len(aSXBI[nFl]) > 4 .And. aSXBI[nFl,5] <> Nil
				SXB->XB_DESCSPA := aSXBI[nFl,5]
			Endif
			If Len(aSXBI[nFl]) > 5 .And. aSXBI[nFl,6] <> Nil
				SXB->XB_DESCENG := aSXBI[nFl,6]
			Endif
			If Len(aSXBI[nFl]) > 6 .And. aSXBI[nFl,7] <> Nil
				SXB->XB_CONTEM  := aSXBI[nFl,7]
			Endif
			If Len(aSXBI[nFl]) > 7 .And. aSXBI[nFl,8] <> Nil
				SXB->XB_WCONTEM := aSXBI[nFl,8]
			Endif
			SXB->(MsUnlock())
		Next nFl
	Endif
	RestArea(aAreaXB)

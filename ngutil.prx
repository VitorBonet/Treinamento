Function NGArrayEmp(bCondic,aCampos)

	Local aRet := {}
	Local cSM0Cd := ""
	Local nX := 0
	Default aCampos := {}
	Default bCondic := {|| .T.}

	dbSelectArea("SM0")
	nRecSM0 := SM0->(RecNo())
	dbGoTop()
	While !Eof()
		If cSM0Cd != SM0->M0_CODIGO
			If Eval(bCondic)
				aAdd(aRet,{SM0->M0_CODIGO,SM0->M0_NOMECOM})
				For nX := 1 To Len(aCampos)
					aAdd(aRet[Len(aRet)],SM0->&(aCampos[nX]))
				Next(nX)
				cSM0Cd := SM0->M0_CODIGO
			EndIf
		EndIf
		dbSelectArea("SM0")
		dbSkip()
	EndDo

	dbGoTo(nRecSM0)

Return aRet


Function NGRETSX9(cAlias,aNao)
	Local aChkSql := {}

	Default aNao := {}

	If cAlias = Nil .Or. Empty(cAlias)
		Return aChkSql
	EndIf

	dbSelectArea("SX9")
	dbSetOrder(1)
	dbSeek(cAlias)
	While !Eof() .And. SX9->X9_DOM = cAlias
		If aScan(aNao,{|x| x == SX9->X9_CDOM}) == 0
			If SX9->X9_ENABLE = 'S'
				cFILDOM  := ""
				cFILCDOM := ""
				If SX9->X9_USEFIL = 'S' .And. !Empty(xFilial(SX9->X9_DOM))
					cFILDOM  := PrefixoCpo(SX9->X9_DOM) +"_FILIAL = '"+ xFilial(SX9->X9_DOM) +"' "
					cFILCDOM := PrefixoCpo(SX9->X9_CDOM)+"_FILIAL = '"+ xFilial(SX9->X9_CDOM)+"' "
				EndIf
				aAdd(aChkSql, {SX9->X9_DOM,AllTrim(SX9->X9_EXPDOM),SX9->X9_CDOM,AllTrim(SX9->X9_EXPCDOM),AllTrim(StrTran(SX9->X9_CONDSQL,"#","")),cFILDOM,cFILCDOM})
			EndIf
		EndIf
		dbSelectArea("SX9")
		dbSkip()
	EndDo

Return aChkSql


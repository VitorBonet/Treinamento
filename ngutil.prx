		cStartPath += cBarras
	EndIf

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Se StartPath NAO tiver barra no inicio, adiciona ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If SubStr(AllTrim(cStartPath),1) != cBarras
		cStartPath = cBarras + cStartPath
	EndIf

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³     Se RootPath tiver barra no final, exclui     ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If SubStr(AllTrim(cRootPath),Len(AllTrim(cRootPath))) == cBarras
		cRootPath = SubStr(AllTrim(cRootPath),1,Len(AllTrim(cRootPath))-1)
	EndIf

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³                Empresa + Filial                  ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If     File(cRootPath+cStartPath+"LGRL"+cSMCOD+cSMFIL+".BMP")
		cLogo := cRootPath+cStartPath+"LGRL"+cSMCOD+cSMFIL+".BMP"

	ElseIf File(cStartPath+"LGRL"+cSMCOD+cSMFIL+".BMP")
		cLogo := cStartPath+"LGRL"+cSMCOD+cSMFIL+".BMP"

	ElseIf File("LGRL"+cSMCOD+cSMFIL+".BMP")
		cLogo := "LGRL"+cSMCOD+cSMFIL+".BMP"

		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³                     Empresa                      ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	ElseIf File(cRootPath+cStartPath+"LGRL"+cSMCOD+".BMP")
		cLogo := cRootPath+cStartPath+"LGRL"+cSMCOD+".BMP"

	ElseIf File(cStartPath+"LGRL"+cSMCOD+".BMP")
		cLogo := cStartPath+"LGRL"+cSMCOD+".BMP"

	ElseIf File("LGRL"+cSMCOD+".BMP")
		cLogo := "LGRL"+cSMCOD+".BMP"

		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³                       Todos                      ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	ElseIf File(cRootPath+cStartPath+"LGRL.BMP")
		cLogo := cRootPath+cStartPath+"LGRL.BMP"

	ElseIf File(cStartPath+"LGRL.BMP")
		cLogo := cStartPath+"LGRL.BMP"

	ElseIf File("LGRL.BMP")
		cLogo := "LGRL.BMP"

	Endif
Return (cLogo)


Function NGDIAUTEIS(_dDataIni,_dDataFim)
	Local aFeriados :=RetFeriados()
	Local nDow
	Local nQtdeDias := 0
	Local dDataWhile := _dDataIni

	While dDataWhile <= _dDataFim

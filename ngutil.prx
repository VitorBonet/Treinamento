*/
Function NGSX3PROXORD(vAlias)
	Local aAreaSX3 := GetArea(),cOrdPr := Space(Len(SX3->X3_ORDEM)),cUlOrd := cOrdPr
	Local aLetNu   := {{'A9','B1'},{'B9','C1'},{'C9','D1'},{'D9','E1'},;
	{'E9','F1'},{'F9','G1'},{'G9','H1'},{'H9','I1'}}
	If NGIFDICIONA("SX3",vAlias,1)
		cUlOrd := SX3->X3_ORDEM
		While !Eof() .And. SX3->X3_ARQUIVO = vAlias
			cUlOrd := Alltrim(SX3->X3_ORDEM)
			Dbskip()
		End
		nPosOrd := aSCAN(aLetNu,{|x| x[1] == cUlOrd})
		If nPosOrd > 0
			cOrdPr := aLetNu[nPosOrd,2]
		ElseIf cUlOrd = '99'
			cOrdPr := "A0"
		Else
			cOrdPr := If(FindFunction("Soma1Old"),PADL(Soma1Old(cUlOrd),2),;
			PADL(Soma1(cUlOrd),2))
		Endif
	Endif
	RestArea(aAreaSX3)
Return cOrdPr

/*/
Static Function CreateSerie(oDlg,nType,aGraph,oObjOpen)
	Local nX,nZ
	Local aTIT   := aGraph[1]
	Local aSER   := aGraph[2]
	//Local aLEG   := aGraph[3]
	Local aEIXOX := aGraph[4]
	Local aVALUE := aGraph[5]
	Local aTot   := {}

	lNGGraLeg  := .T.
	nNGGraType := nType

	RELEASE OBJECTS oNGGraphic
	If oObjOpen != Nil
		//	oObjOpen:FreeChildren()
	Endif
	oNGGraphic := TMSGraphic():New( 01,01,IIf(oObjOpen != Nil,oObjOpen,oDlg),,,RGB(239,239,239),260,184)
	oNGGraphic:Hide()
	oNGGraphic:SetMargins(6,6,6,6)
	oNGGraphic:Align := CONTROL_ALIGN_ALLCLIENT
	oNGGraphic:l3D   := .T.
	//oNGGraphic:SetGradient( GDBOTTOMTOP, CLR_HGRAY, CLR_BLACK)

	If !Empty(aTIT[3]+aTIT[2])
		oNGGraphic:SetTitle( aTIT[2],aTIT[3],aColorText[1],A_CENTER,GRP_TITLE)
	EndIf
	If !Empty(aTIT[4])
		oNGGraphic:SetTitle( ,aTIT[4]    ,aColorText[2], A_CENTER , GRP_FOOT)
	EndIf

	ProcRegua(Len(aSER)*Len(aEIXOX))

	If GRP_PIE == nType
		nTotal := 0
		aEval(aVALUE[1],{|x| nTotal += x})
		If Len(aSER) > 1 .or. nTotal == 0
			nTotal := 0
			For nZ := 1 To Len(aSER)
				nSerie   := oNGGraphic:CreateSerie(nType)
				nTotSerie:= 0
				For nX := 1 To Len(aEIXOX)
					IncProc(STR0167) //"Processando gráfico..."
					nTotSerie += aVALUE[nZ][nX]
					nTotal    += aVALUE[nZ][nX]
				Next nX
				aAdd(aTot,{nSerie,nTotSerie})
			Next nZ
			nTotal /= 100
			For nZ := 1 To Len(aTot)
				oNGGraphic:Add(aTot[nZ][1], Round(aTot[nZ][2]/nTotal,2), AllTrim(aSER[nZ]), aColorGraph[nZ] )
			Next nZ
		Else
			nSerie := oNGGraphic:CreateSerie(nType)
			nTotal /= 100
			For nX := 1 To Len(aEIXOX)
				If Len(aEIXOX) > 16 .and. aVALUE[1][nX] == 0
					Loop
				Endif
				IncProc(STR0167) //"Processando gráfico..."
				xCorVar := 0
				If nX > Len(aColorGraph)
					xCorVar := RGB( Randomize(0,255) , Randomize(0,255) , Randomize(0,255) )
				Else
					xCorVar := aColorGraph[nX]
				Endif
				oNGGraphic:Add(nSerie, Round(aVALUE[1][nX]/nTotal,2), aEIXOX[nX], xCorVar )
			Next nX
		EndIf
	Else
		For nZ := 1 To Len(aSER)
			nSerie   := oNGGraphic:CreateSerie(nType,AllTrim(aSER[nZ]))
			For nX := 1 To Len(aEIXOX)
				IncProc(STR0167) //"Processando gráfico..."
				oNGGraphic:Add(nSerie, aVALUE[nZ][nX], aEIXOX[nX], aColorGraph[nZ] )
			Next nX
		Next nZ
	EndIf
	oNGGraphic:Show()
Return


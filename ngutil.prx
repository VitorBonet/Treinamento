Static Function fVersFonte(cLinha)

	Local nCont
	Local cVers

	cLinha := AllTrim(cLinha)
	cVers  := ""

	For nCont := 1 To Len(cLinha)
		If NGIsAllNum( SubStr(cLinha,nCont,1),1 )
			cVers += SubStr(cLinha,nCont,1)
		ElseIf !Empty(cVers)
			Exit
		EndIf
	Next nCont

	If !Empty(cVers)
		cVers := StrZero( Val(cVers) , 3 )
	EndIf

Return cVers


FUNCTION HTOM(cTIME)
	LOCAL nHORA,nMINUTO,POS

	POS := At(":",cTIME)
	IF POS == 0;RETURN 0;END
	nHORA   := VAL(SUBSTR(cTIME,1,(POS-1)))
	nMINUTO := VAL(SUBSTR(cTIME,(POS+1)))
RETURN (nHORA*60)+nMINUTO


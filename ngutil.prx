Function NGEMPFIELDS(cAlias,aEmp,lComum)
	Local nX, nQtd
	Local aPSX3   := {}
	Local aDSX3   := {}
	Local aComum  := {}
	Local aDifere := {}
	Local aX3Area := SX3->(GetArea())
	Local aM0Area := SM0->(GetArea())

	Default cAlias := Alias()
	Default aEmp   := {}
	Default lComum := .T.

	cAlias := Upper(cAlias)

	If Empty(cAlias)
		Return {}
	EndIf

	//Se estiver vazia, pega todas as empresas
	If Len(aEmp) == 0
		NGDBAREAORDE("SM0",1)
		dbGoTop()
		While !Eof()
			If aSCAN(aEmp,{|x| x == SM0->M0_CODIGO}) == 0
				aAdd(aEmp,SM0->M0_CODIGO)
			EndIf
			dbSkip()
		End
	EndIf

	//Abre SX3 da primeira empresa e adiciona campos na primeira array
	If NGABRESX("SX3",aEmp[1])
		NGIFDICIONA("SX3",cAlias,1,.F.)
		While !Eof() .And. SX3->X3_ARQUIVO == cAlias
			If SX3->X3_CONTEXT <> "V"
				aAdd(aPSX3,SX3->X3_CAMPO)
			Endif
			dbSkip()
		End

		//Abre SX3 das demais empresas e adiciona campos em outra array
		For nX := 2 to Len(aEmp)
			If NGABRESX("SX3",aEmp[nX])
				NGIFDICIONA("SX3",cAlias,1,.F.)
				While !Eof() .And. SX3->X3_ARQUIVO == cAlias
					If SX3->X3_CONTEXT <> "V"
						aAdd(aDSX3,SX3->X3_CAMPO)
					Endif
					dbSkip()
				End
			EndIf
		Next nX

		//Verifica quais campos sao comuns ente as empresas
		For nX := 1 To Len(aPSX3)
			nQtd := 0
			aEval(aDSX3, {|x| If(x == aPSX3[nX], nQtd++, Nil) })
			If nQtd == Len(aEmp)-1
				If lComum
					aAdd(aComum,aPSX3[nX])
				EndIf
			Else
				If !lComum
					aAdd(aDifere,aPSX3[nX])
				EndIf
			EndIf
		Next nX

		//Retorna SX3 da empresa local
		NGABRESX("SX3",cEmpAnt)
	EndIf

	RestArea(aM0Area)
	RestArea(aX3Area)

Return If(lComum,aComum,aDifere)


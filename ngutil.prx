/*/
Static Function NGVERPROC()
	Local cCamDir := Alltrim(cDiret), cFIMLI := chr(13)+chr(10),nF1 := 0
	Local cVersFonte := ""
	Local nLenFonte  := 0
	cVersRPO      := Alltrim(cVersao)
	cVersRpo      := STRTRAN(cVersRPO,".","")

	nPos2 := At(":",cCamDir)
	If Empty(nPos2)
		cCamDir := "C:\" + cCamDir
	Endif
	If SubStr(cCamDir,Len(cCamDir),1) <> "\"
		cCamDir += "\"
	Endif

	cArqTXN := "NGVER"+cVersRPO
	cArqTXT := "NGVER"+cVersRPO+".TXT"

	If File(cArqTXT)
		Ferase(cArqTXT)
	Endif

	vRetTXT := NGCRIATXTCAB(,,cArqTXN)
	If vRetTXT[1]
		nLogHdl := vRetTXT[2]
		cString := Space(50)+cFIMLI
		For nF1 := 1 To 2
			FWrite(nLogHdl,cString,Len(cString))
		Next nF1
	Else
		MsgInfo(STR0259+" "+cArqTXT,STR0215) //"Problema na criacao do"###"NAO CONFORMIDADE"
		FClose(nLogHdl)
		DbCommit()
		Ferase(cArqTXT)
		Return .F.
	Endif

	FWrite(nLogHdl,STR0241+Chr(9)+Chr(9)+STR0125+Chr(9)+Chr(9)+STR0129+Chr(9)+STR0260+cFIMLI) //"Fonte"###"Data"###"Hora"###"Versão"

	aPROGS := Directory(cCamDir+"*.*")
	MakeDir("\NGVERFONTE")
	ProcRegua(len(aPROGS))

	For nF1 := 1 To Len(aPROGS)
		IncProc()
		If ".PRX" $ Upper(aPROGS[nF1,1]) .Or. ".PRW" $ Upper(aPROGS[nF1,1])
			If Upper(SubStr(aPROGS[nF1,1],1,2)) == "NG" .Or. Upper(SubStr(aPROGS[nF1,1],1,3)) == cVarMod
				cVersFonte := ""
				CpyT2S(cCamDir+aPROGS[nF1,1], "\NGVERFONTE") //Cria um arquivo copia do fonte, no servidor
				FT_FUse("\NGVERFONTE\"+aPROGS[nF1,1])
				While !FT_FEof()
					If "#DEFINE" $ Upper(FT_FReadLn()) .And. "VERSAO" $ Upper(FT_FReadLn())
						cVersFonte := fVersFonte(FT_FReadLn())
						If !Empty(cVersFonte)
							Exit
						EndIf
					EndIf
					FT_FSkip()
				End
				FT_FUse()
				nLenFonte := Len(Alltrim(aPROGS[nF1,1]))
				FErase("\NGVERFONTE\"+aPROGS[nF1,1]) //Deleta o arquivo criado
				cString := Alltrim(aPROGS[nF1,1])
				If nLenFonte <= 7
					cString += Chr(9)+Chr(9)
				Else
					cString += Chr(9)
				EndIf
				cString += Dtoc(aPROGS[nF1,3])+Chr(9)+SubStr(aPROGS[nF1,4],1,5)+Chr(9)+cVersFonte+Chr(9)+cFIMLI
				FWrite(nLogHdl,cString,Len(cString))
				nQtdP ++
			Endif
		Endif
	Next nF1
	FClose(nLogHdl)
	DbCommit()
Return


*/
Function NGTransInd(nInd)

	Local cInd     := ""
	Local nCont    := 0
	Local lCont    := .T.

	Default nInd := 0

	If nInd >= 10
		cInd  := "1"
		While lCont
			nCont++

			If nCont == 10
				cInd := "A"
			EndIf

			If nCont == nInd
				lCont := .F.
				Exit
			EndIf

			cInd := Soma1(cInd)
		End
	Else
		cInd := AllTrim(Str(nInd))
	EndIf

Return cInd


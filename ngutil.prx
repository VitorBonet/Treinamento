/*/
Function NGABRESX(cArquivo,cEmp)

	Local cAliSX, cSX, cIndSX, nInd
	Local lReturn := .F.

	If cEmp == NIL .Or. Empty(cEmp)
		cEmp := SM0->M0_CODIGO
	Endif

	If cArquivo $ "SIX/SX1/SX2/SX3/SX6/SX7/SXB"

		aInd := {{IIF(File("SINDEX"+GetDbExtension()),"SINDEX","SIX"),"INDICE+ORDEM"},;
		{"SX1","X1_GRUPO+X1_ORDEM","X1_IDFIL+X1_ORDEM"},;
		{"SX2","X2_CHAVE"},;
		{"SX3","X3_ARQUIVO+X3_ORDEM","X3_CAMPO","X3_GRPSXG+X3_ARQUIVO","X3_ARQUIVO+X3_FOLDER","X3_ARQUIVO+X3_IDXFLD"},;
		{"SX6","X6_FIL+X6_VAR"},;
		{"SX7","X7_CAMPO+X7_SEQUENC"},;
		{"SXB","XB_ALIAS+XB_TIPO+XB_SEQ+XB_COLUNA"},}
		cAliSX := cArquivo
		If cArquivo == "SIX" .And. File("SINDEX"+GetDbExtension())
			cSX := "SINDEX"
			cInd := "SINDEX"
			cArquivo := "SINDEX"
		Else
			cSX := cArquivo+cEmp+'0'
			cInd := cSX
		Endif

		//Fecha alias corrente
		dbSelectArea(cAliSX)
		USE

		//Abre alias da empresa com o mesmo 'nome'
		If File(cSX+GetDbExtension())
			MsOpEndbf(.T.,__LocalDriver,cSX,cAliSX,.T.,.F.)
			DbSelectArea(cAliSX)
			For nInd := 2 To Len(aInd[aSCAN(aInd, {|x| x[1] == cArquivo})])
				If Empty(IndexKey(nInd-1))
					MsOpenIdx(cInd+AllTrim(STR(nInd-1)),aInd[aSCAN(aInd, {|x| x[1] == cArquivo}),nInd],.T.,,,cInd)
					dbSetIndex(cInd)
				EndIf
			Next nInd
			dbSetOrder(01)
			lReturn := .T.
		EndIf

	EndIf

Return lReturn


Function NGCHKFODIF(cArq1TXT,cArq2TXT,nDias)
	Local nCont := 0,aArCLX,aArNg,lRet := .T., cCont := "",aContA := {}
	nDiasAt := If(nDias = Nil,0,nDias)
	aArCLX  := {}
	nHdlArq := FOPEN(cArq1TXT,0)
	nTamArq := fseek(nHdlArq,0,2)

	FT_FUSE(cArq1TXT)
	FT_FGOTOP()
	If nModulo == 19 //MNT
		While (!FT_FEof())
			cLinha := FT_FREADLN()
			If !Empty(cLinha)
				If SubStr(cLinha,1,2) = 'NG' .Or. SubStr(cLinha,1,3) = 'MNT'
					nPosB := At(' ',cLinha)
					If nPosB > 0
						Aadd(aArCLX,{Substr(cLinha,1,nPosB-1),;
						Ctod(Alltrim(Substr(cLinha,nPosB+1,10))),;
						Alltrim(Substr(cLinha,nPosB+10,5))})
					Endif
				Endif
			Endif
			FT_FSKIP()
		End
	ElseIf nModulo == 35 //MDT
		While (!FT_FEof())
			cLinha := FT_FREADLN()
			If !Empty(cLinha)
				If SubStr(cLinha,1,2) = 'NG' .Or. SubStr(cLinha,1,3) = 'MDT'
					nPosB := At(' ',cLinha)
					If nPosB > 0
						Aadd(aArCLX,{Substr(cLinha,1,nPosB-1),;
						Ctod(Alltrim(Substr(cLinha,nPosB+1,10))),;
						Alltrim(Substr(cLinha,nPosB+10,5))})
					Endif
				Endif
			Endif
			FT_FSKIP()
		End
	ElseIf nModulo == 56 //SGA
		While (!FT_FEof())
			cLinha := FT_FREADLN()
			If !Empty(cLinha)
				If SubStr(cLinha,1,2) = 'NG' .Or. SubStr(cLinha,1,3) = 'SGA'
					nPosB := At(' ',cLinha)
					If nPosB > 0
						Aadd(aArCLX,{Substr(cLinha,1,nPosB-1),;
						Ctod(Alltrim(Substr(cLinha,nPosB+1,10))),;
						Alltrim(Substr(cLinha,nPosB+10,5))})
					Endif
				Endif
			Endif
			FT_FSKIP()
		End
	EndIf
	FT_FUSE()
	fClose(nhdlArq)

	aArNg   := {}
	aContA  := {}
	cCont   := ""
	nHdlAr2 := FOPEN(cArq2TXT,0)
	nTamAr2 := fseek(nHdlAr2,0,2)

	FT_FUSE(cArq2TXT)
	FT_FGOTOP()
	If nModulo == 19
		While (!FT_FEof())
			cLinha := FT_FREADLN()
			cCont += cLinha+CRLF
			If !Empty(cLinha)
				If SubStr(cLinha,1,2) = 'NG' .Or. SubStr(cLinha,1,3) = 'MNT'
					nPosB := At(' ',cLinha)
					If nPosB > 0
						Aadd(aArNg,{Substr(cLinha,1,nPosB-1),;
						Ctod(Alltrim(Substr(cLinha,nPosB+1,10))),;
						Alltrim(Substr(cLinha,nPosB+10,5))})
					Endif
				Endif
			Endif
			FT_FSKIP()
		End
	ElseIf nModulo == 35
		While (!FT_FEof())
			cLinha := FT_FREADLN()
			cCont += cLinha+CRLF
			If !Empty(cLinha)
				If SubStr(cLinha,1,2) = 'NG' .Or. SubStr(cLinha,1,3) = 'MDT'
					nPosB := At(' ',cLinha)
					If nPosB > 0
						Aadd(aArNg,{Substr(cLinha,1,nPosB-1),;
						Ctod(Alltrim(Substr(cLinha,nPosB+1,10))),;
						Alltrim(Substr(cLinha,nPosB+10,5))})
					Endif
				Endif
			Endif
			FT_FSKIP()
		End
	ElseIf nModulo == 56
		While (!FT_FEof())
			cLinha := FT_FREADLN()
			cCont += cLinha+CRLF
			If !Empty(cLinha)
				If SubStr(cLinha,1,2) = 'NG' .Or. SubStr(cLinha,1,3) = 'SGA'
					nPosB := At(' ',cLinha)
					If nPosB > 0
						Aadd(aArNg,{Substr(cLinha,1,nPosB-1),;
						Ctod(Alltrim(Substr(cLinha,nPosB+1,10))),;
						Alltrim(Substr(cLinha,nPosB+10,5))})
					Endif
				Endif
			Endif
			FT_FSKIP()
		End
	EndIf
	FT_FUSE()
	fClose(nHdlAr2)
	For nCont := 1 To Len(aArNg)
		nPosA := aSCAN(aArCLX, {|x| x[1] == aArNg[nCont,1]})
		If nPosA = 0
			lRet := .F.
		Else
			If aArCLX[nPosA,2] < aArNg[nCont,2]
				lRet := .F.
			Else
				If aArCLX[nPosA,2] > aArNg[ncont,2]
					Aadd(aContA,{aArNg[nCont,1]+" "+Dtoc(aArNg[nCont,2]),;
					aArCLX[nPosA,1]+" "+Dtoc(aArCLX[nPosA,2])})
				Endif
			Endif
		Endif
	Next nCont

	If Len(aContA) > 0
		For nCont := 1 to Len(aContA)
			cCont := StrTran(cCont,aContA[nCont,1],aContA[nCont,2])
		Next nCont
		If File(cArq2TXT)
			Ferase(cArq2TXT)
		Endif

		_nHdl := Fcreate(cArq2TXT)

		fWrite(_nHdl,cCont,Len(cCont))

		fClose(_nHdl)

	Endif
Return lRet


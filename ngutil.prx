Function NGGraphic(nType,oObjOpen,_oObjLeg)
	Local oDlg
	Local cTitle   := ""


	//Variaveis de Largura/Altura da Janela
	Local nAltura  := (GetScreenRes()[2]-85)
	Local nLargura := (GetScreenRes()[1]-7)

	Default oObjOpen := Nil
	Default _oObjLeg := Nil
	Default nType := GRP_BAR //GRP_LINE, GRP_AREA, GRP_POINT, GRP_BAR, GRP_PIE

	Private nNGGraType := nType
	Private lNGGraLeg  := .T.
	Private aGraph   := ReadNgGrafi()

	If oObjOpen == Nil
		Private oNGGraphic
		//Cores disponiveis para o grafico
		Private aColorGraph := {CLR_HGREEN,CLR_HBLUE,CLR_HRED,CLR_YELLOW,CLR_BROWN,CLR_CYAN,;
		CLR_MAGENTA,CLR_GRAY,CLR_WHITE,CLR_BLUE,CLR_GREEN,CLR_RED,CLR_HGRAY,;
		CLR_HCYAN,CLR_HMAGENTA,CLR_BLACK,RGB(178,241,18),RGB(131,199,154),;
		RGB(111,156,210),RGB(236,99,120),RGB(255,255,162),RGB(19,88,88),;
		RGB(60,9,60),RGB(141,125,194),RGB(255,93,0),RGB(255,196,0)}
	Endif

	//Se nao foi gerado relatorio retorna .F.
	If Len(aGraph) == 0
		Return .F.
	EndIf








	//Cores para os textos do cabecalho e rodape
	Private aColorText  := {CLR_GREEN,CLR_GREEN}

	cTitulo := aGraph[6]

	If oObjOpen != Nil
		Processa({|| CreateSerie(oObjOpen,nNGGraType,aGraph)})
		MenuGraph(oDlg,aGraph,_oObjLeg,oObjOpen)
	Else
		DEFINE DIALOG oDlg TITLE cTitulo FROM 120,0 TO nAltura,nLargura PIXEL
		oDlg:lMaximized := .T.
		oDlg:lEscClose  := .F.
		Processa({|| CreateSerie(oDlg,nNGGraType,aGraph)})
		ACTIVATE DIALOG oDlg ON INIT (MenuGraph(oDlg,aGraph))// CENTERED
	Endif
Return .T.


/*/
Function NGSEMANANO(dDTANO)
	Local i
	Local nSemana := 0
	Local dData
	Local nData

	If !Empty(dDTANO)

		dData := CTOD('01/01/'+Right(cValToChar(Year(dDTANO)),2))

		If dow(dData) <> 1
			For i := 1 to 6
				dData := dData + 1
				If Dow(dData) == 1
					Exit
				Endif
			Next
		Endif

		nData := Int((dDTANO - dData) / 7) + 1

		nSemana := Int(nData)
	Endif
Return nSemana


Function NGRETORDEM(cAliTab,cConte,lExato)
	Local lPosic  := If(lExato = Nil,.T.,lExato)
	Local aAreaId := GetArea(), nIndRet := 0, nIndInc := 0
	DbSelectArea("SIX")
	DbSetOrder(1)
	If Dbseek(cAliTab)
		While !Eof() .And. SIX->INDICE = cAliTab
			nIndInc ++
			If lPosic
				If Alltrim(StrTran(Upper(SIX->CHAVE)," ","")) = Alltrim(StrTran(Upper(cConte)," ",""))
					nIndRet := nIndInc
					Exit
				EndIf
			Else
				If Alltrim(StrTran(Upper(cConte)," ","")) $ StrTran(Upper(SIX->CHAVE)," ","")
					nIndRet := nIndInc
					Exit
				Endif
			Endif
			Dbskip()
		End
	Endif
	RestArea(aAreaId)
Return nIndRet


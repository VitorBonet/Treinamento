	@ 67,060 GET cDiret SIZE 80,10 OF oDLGA PIXEL
	@ 67,150 BUTTON STR0256 SIZE 32,12 ACTION (cDiret := NGRetBusca(1,,,,cDiret)) OF oDLGA PIXEL //"Localizar" //"Localizar"

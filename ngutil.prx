	aAdd( aColsFil , fObjColFWB( "{|| aFiliais[oBrowse:At()]["+cValToChar( 2 )+"] }",CONTROL_ALIGN_LEFT,.F.,STR0154,"C",15,"@" ) )  // "Filial"

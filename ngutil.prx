/*/
Function NGRETPARSX6(cVPar,cVCond,cFilAc,cEmpAc)
	Local aAreaPa   := GetArea()
	Local lTrocaEmp := .F.
	Local uVRet
	// Armazena a Filial
	Local cAuxFil   := cFilAnt

	Default cFilAc := cFilAnt

	cFilAnt := cFilAc

	//Se for outra empresa cria alias temporario
	If cEmpAc <> NIL .And. cEmpAnt != cEmpAc
		NGABRESX("SX6",cEmpAc)
		lTrocaEmp := .T.
	EndIf

	//Verifica se o parametro existe
	uVRet := GetNewPar(cVPar,"_%*ERR*%_",cFilAc)
	If ValType(uVRet) == "C" .And. uVRet == "_%*ERR*%_"
		If lTrocaEmp
			NGABRESX("SX6",cEmpAnt)
		EndIf

		// Retorna a Filial
		cFilAnt := cAuxFil

		Return Nil
	Else
		uVRet := GetMv(cVPar,.F.,"_%*ERR*%_")
	EndIf

	If ValType(cVCond) != "U"
		If ValType(cVCond) == ValType(uVRet)
			uVRet := uVRet == cVCond
		Else
			uVRet := .F.
		EndIf
	EndIf

	If lTrocaEmp
		NGABRESX("SX6",cEmpAnt)
	EndIf

	// Retorna a Filial
	cFilAnt := cAuxFil

	RestArea(aAreaPa)
Return uVRet

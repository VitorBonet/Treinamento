Function NGRETCPEMP(cArquivo,nOrdem,cCampo,cChave,cEmp,lLang)

	Local aArea := GetArea()
	Local cRetorno
	Local cSX, nInd
	Local cAliSX
	Local aCpos := {{"DESCRICAO","DESCSPA","DESCENG"},;
	{"X1_PERGUNT","X1_PERSPA","X1_PERENG"},;
	{"X1_DEF01","X1_DEFSPA1","X1_DEFENG1"},;
	{"X1_DEF02","X1_DEFSPA2","X1_DEFENG2"},;
	{"X1_DEF03","X1_DEFSPA3","X1_DEFENG3"},;
	{"X1_DEF04","X1_DEFSPA4","X1_DEFENG4"},;
	{"X1_DEF05","X1_DEFSPA5","X1_DEFENG5"},;
	{"X2_NOME","X2_NOMESPA","X2_NOMEENG"},;
	{"X3_TITULO","X3_TITSPA","X3_TITENG"},;
	{"X3_DESCRIC","X3_DESCSPA","X3_DESCENG"},;
	{"X3_CBOX","X3_CBOXSPA","X3_CBOXENG"},;
	{"X6_DESCRIC","X6_DESCSPA","X6_DESCENG"},;
	{"X6_DESC1","X6_DSCSPA1","X6_DSCENG1"},;
	{"X6_DESC2","X6_DSCSPA2","X6_DSCENG2"},;
	{"X6_CONTEUD","X6_CONTSPA","X6_CONTENG"},;
	{"X6_DEFPOR","X6_DEFSPA","X6_DEFENG"}}

	nOrdem := If(nOrdem == NIL, 1, nOrdem)
	cEmp := If(cEmp == NIL, cEmpAnt,cEmp)
	lLang := If(lLang == NIL,.T.,lLang)

	//Busca campo correspondente ao Idioma utilizado
	If lLang
		nLang := 1
		#IFDEF SPANISH
		nLang := 2
		#ELSE
		#IFDEF ENGLISH
		nLang := 3
		#ENDIF
		#ENDIF
		nCpos := aSCAN(aCpos, {|x| cCampo $ x[1]+"/"+x[2]+"/"+x[3]})
		If nCpos > 0
			cCampo := aCpos[nCpos,nLang]
		EndIf
	EndIf

	If cArquivo $ "SIX/SX1/SX2/SX3/SX6/SX7/SXB"

		If (cEmpAnt == cEmp)
			dbSelectArea(cArquivo)
			dbSetOrder(nOrdem)
			cRetorno := If(dbSeek(cChave),(cArquivo)->&(cCampo),NIL)
		Else

			aInd := {{IIF(File("SINDEX"+GetDbExtension()),"SINDEX","SIX"),"INDICE+ORDEM"},;
			{"SX1","X1_GRUPO+X1_ORDEM","X1_IDFIL+X1_ORDEM"},;
			{"SX2","X2_CHAVE"},;
			{"SX3","X3_ARQUIVO+X3_ORDEM","X3_CAMPO","X3_GRPSXG+X3_ARQUIVO","X3_ARQUIVO+X3_FOLDER","X3_ARQUIVO+X3_IDXFLD"},;
			{"SX6","X6_FIL+X6_VAR"},;
			{"SX7","X7_CAMPO+X7_SEQUENC"},;
			{"SXB","XB_ALIAS+XB_TIPO+XB_SEQ+XB_COLUNA"},}
			cAliSX := "ALISX"
			If cArquivo == "SIX" .And. File("SINDEX"+GetDbExtension())
				cSX := "SINDEX"
				cInd := "SINDEX"
				cArquivo := "SINDEX"
			Else
				cSX := cArquivo+cEmp+'0'
				cInd := cSX
			Endif

			If File(cSX+GetDbExtension())
				MsOpEndbf(.T.,__LocalDriver,cSX,cAliSX,.T.,.F.)
				DbSelectArea(cAliSX)
				If Empty(IndexKey(nOrdem))
					For nInd := 2 To Len(aInd[aSCAN(aInd, {|x| x[1] == cArquivo})])
						MsOpenIdx(cInd+AllTrim(STR(nInd-1)),aInd[aSCAN(aInd, {|x| x[1] == cArquivo}),nInd],.T.,,,cInd)
						dbSetIndex(cInd)
					Next nInd
				EndIf

				dbSetOrder(nOrdem)
				cRetorno := If(dbSeek(cChave),(cAliSX)->&(cCampo),NIL)

				dbSelectArea(cAliSX)
				USE
			EndIf
		EndIf

	EndIf

	RestArea(aArea)

Return cRetorno


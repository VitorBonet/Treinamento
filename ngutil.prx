±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
/*/
Function NGGRAFICO(cTITJA,cTITEG,cTITSG,cTITDG,cTITIN,vSERIE,cLEIT,cVALIA,;
	aMATRI, cTypeGraf, lNgGrafico, oObjOpen, oObjLeg)
	Local cFIMLI    := chr(13)+chr(10),nv,vn,xx,xa,cTXTGRAF := "NGGRAFI.TXT"
	Local cMENTXT   := STR0108+chr(13)+STR0109+chr(13)+chr(13)+STR0110+" NGCRIAGATXT"
	Local lFUNCOK   := .T., cPARGRAF := "c:\grafing\"
	Local dTGRAEXE  := Ctod("04/09/06"),dTEXEST := Ctod("  /  /  "),dTEXETA := dTEXEST
	Local vARQEXE   := {"GRAFING.EXE","CHART2FX.VBX","BIVBX10.DLL","CTL3D.DLL"}
	Local cBARRAS   := If(isSRVunix(),"/","\"),cBARRAD := If(isSRVunix(),"//","\\")
	Local cROOTPATH := Alltrim(GetSrvProfString("RootPath",cBARRAS) )
	Local cSTARPATH := AllTrim(GetSrvProfString("StartPath",cBARRAS) )
	Local cDIREXETH := cROOTPATH+cSTARPATH
	Local cROOTPAT2 := If(Substr(cROOTPATH,Len(cROOTPATH),1) <> cBARRAS,;
	cROOTPATH+cBARRAS,cROOTPATH)
	Local cARQUIAUX := "GRAFING.EXE , CHART2FX.VBX , BIVBX10.DLL , CTL3D.DLL"
	Local nType
	Local lLinux   := GetRemoteType() == 2
	Local l64Bits  := .F.
    Local aARQAUX1 := {} //Array para busca de arquivos contidos no vetor -> vARQEXE
	Default lNgGrafico := .F.

	cARQUIAUX := If(IsSRVunix(),Lower(cARQUIAUX),cARQUIAUX)

	If cLEIT = "A" .And. (cVALIA = NIL .Or. Empty(cVALIA))
		lFUNCOK := .F.
	Elseif cLEIT = "M" .And. (aMATRI = NIL .Or. Empty(aMATRI))
		lFUNCOK := .F.
	Endif

	If !lFUNCOK
		MsgInfo(cMENTXT,STR0053)
		Return .F.
	Endif

	cDIREXETH := Strtran(cDIREXETH,cBARRAD,cBARRAS)
	cDIRCRIAR := If(Substr(cPARGRAF,Len(cPARGRAF),1) = cBARRAS,;
	Substr(cPARGRAF,1,Len(cPARGRAF)-1),cPARGRAF)

	cROOTPAT2 := If(Substr(cROOTPAT2,Len(cROOTPAT2),1) <> cBARRAS,;
	cROOTPAT2+cBARRAS,cROOTPAT2)
	cSTARPATH := If(Substr(cSTARPATH,Len(cSTARPATH),1) <> cBARRAS,;
	cSTARPATH+cBARRAS,cSTARPATH)
	cDIREXETH := If(Substr(cDIREXETH,Len(cDIREXETH),1) <> cBARRAS,;
	cDIREXETH+cBARRAS,cDIREXETH)

	cDIRARQA := cROOTPAT2+cSTARPATH
	cDIRARQA := Strtran(cDIRARQA,cBARRAD,cBARRAS)

	aARQAUXF := {}

	//Inserida validação para linux pois o mesmo não utiliza os arquivos contidos no vARQEXE
	If !lLinux

		For xx := 1 To Len(vARQEXE)

			aARQAUX1 := Directory(vARQEXE[xx])
			nPOS := Ascan(aARQAUX1,{|x| x[1] == vARQEXE[xx]})

			If nPOS > 0
				Aadd(aARQAUXF,aARQAUX1[nPOS])
			Else
				MsgInfo(STR0075+" "+vARQEXE[xx]+STR0076+chr(13)+chr(13);
				+STR0104+chr(13);
				+"GRAFING.EXE, CHART2FX.VBX, BIVBX10.DLL e CTL3D.DLL."+chr(13)+chr(13);
				+STR0105+cDIRARQA+chr(13);
				+STR0106,STR0053)
				Return .F.
			Endif
		Next xx

		If aARQAUXF[1,3] < dTGRAEXE
			MsgInfo(vARQEXE[1]+" "+STR0093+chr(13)+chr(13);
			+STR0090+chr(13)+STR0094+" "+vARQEXE[1]+".",STR0053)
			Return .F.
		Endif
		xx := 0
		For xx := 1 To Len(aARQAUXF)
			Ferase(cPARGRAF+aARQAUXF[xx][1])
		Next xx
		MAKEDIR(cDIRCRIAR)

		For xa := 1 To Len(aARQAUXF)
			__copyfile(aARQAUXF[xa,1],cPARGRAF+aARQAUXF[xa,1])
		Next xa

		aEXEATRIA := Directory(cPARGRAF+"*.*")
		xx := 0

		For xx := 1 To Len(vARQEXE)
			If Ascan(aEXEATRIA,{|x| x[1] == vARQEXE[xx]}) == 0
				MsgInfo(STR0075+" "+vARQEXE[xx]+" "+STR0076+STR0107+cPARGRAF+chr(13)+chr(13);
				+STR0104+chr(13);
				+cARQUIAUX+"."+chr(13)+chr(13);
				+STR0106,STR0053)
				Return .F.
			Endif
		Next xx

		aEXEATRIA := Directory(cPARGRAF+aARQAUXF[1,1])

		If Len(aEXEATRIA) > 0
			dTEXETA := aEXEATRIA[1,3]
			If dTEXETA < aARQAUXF[1,3]
				Ferase(cPARGRAF+aARQAUXF[1,1])
				__copyfile(aARQAUXF[1,1],cPARGRAF+aARQAUXF[1,1])
			Endif
		EndIf

		If file(cTXTGRAF)
			Ferase(cTXTGRAF)
		Endif
	EndIf

	nLogHdl := MsfCreate(cTXTGRAF)
	If ferror() # 0 .Or. nLogHdl < 0
		MsgInfo(STR0102,STR0025)
		FClose(nLogHdl)
		DbCommit()
		Ferase(cTXTGRAF)
		Return .F.
	Endif

	// 1§ linha titulo do grafico (janela)
	cString  := cTITJA+cFIMLI
	FWrite(nLogHdl,cString,Len(cString))

	// 2§ linha titulo da esquerda do grafico
	cString  := cTITEG+cFIMLI
	FWrite(nLogHdl,cString,Len(cString))

	// 3§ linha titulo superior do grafico
	cString  := cTITSG+cFIMLI
	FWrite(nLogHdl,cString,Len(cString))

	// 4§ linha titulo da direita do grafico
	cString  := cTITDG+cFIMLI
	FWrite(nLogHdl,cString,Len(cString))

	// 5§ linha titulo da inferior do grafico
	cString  := cTITIN+cFIMLI
	FWrite(nLogHdl,cString,Len(cString))

	// 6§ linha series do grafico
	cString := Space(1)
	For nv := 1 To Len(vSERIE)
		cString := Alltrim(cString)+vSERIE[nv]
		If nv < Len(vSERIE)
			cString := cString + '#'
		Endif
	Next nv
	cString := cString + cFIMLI
	FWrite(nLogHdl,cString,Len(cString))

	// Linhas do conteudo dos dados
	If cLEIT = "A"
		DbselectArea(cVALIA)
		Dbgotop()
		While !eof()
			cString := Space(1)
			For nv := 1 To Fcount()
				ny := cVALIA+"->" + Fieldname(nv)
				If nv < 3
					cString := Alltrim(cString)+Alltrim(&(ny))+"#"
				Else
					cString := cString+Alltrim(Str(&(ny),12,2))
					If nv < Fcount()
						cString := cString+";"
					Endif
				Endif
			Next nv
			cString := cString + cFIMLI
			FWrite(nLogHdl,cString,Len(cString))

			DbselectArea(cVALIA)
			Dbskip()
		End
	Else
		For nv := 1 To Len(aMATRI)
			cString := Space(1)
			For vn := 1 To Len(aMATRI[nv])
				If vn < 3
					cString := Alltrim(cString)+Alltrim(aMATRI[nv,vn])+"#"
				Else
					cString := cString+Alltrim(Str(aMATRI[nv,vn],12,2))
					If nv < Fcount()
						cString := cString+";"
					Endif
				Endif
			Next vn
			cString := cString + cFIMLI
			FWrite(nLogHdl,cString,Len(cString))
		Next nv
	Endif
	FClose(nLogHdl)
	DbCommit()

	__copyfile(cTXTGRAF,cPARGRAF+cTXTGRAF)

	If !lLinux
		cPath   := GetEnv("PATH")
		nPath   := At("\WINDOWS\",Upper(cPath))
		l64Bits := ExistDir(Substr(cPath,nPath-2,3)+"Windows\SysWOW64")
	EndIf

	If lLinux .Or. l64Bits .Or. lNgGrafico .Or. MsgYesNo(STR0166,STR0025) //"Deseja visualizar o gráfico em ambiente Protheus?"##"ATENCAO"
		nType := Nil
		If !Empty(cTypeGraf)
			If cTypeGraf $ "025"
				nType := GRP_LINE
			ElseIf cTypeGraf == "4"
				nType := GRP_PIE
			ElseIf  cTypeGraf $ "37"
				nType := GRP_POINT
			Else
				nType := GRP_BAR
			EndIf
		EndIf
		If IsInCallStack("MNTC920")
			NGGRAFFWCH()
		ElseIf IsInCallStack("MNTC910")
			NGGRAFFMOV()
		Else
			NGGRAPHIC(nType,IIF(lNgGrafico,oObjOpen,),IIF(lNgGrafico,oObjLeg,))
		EndIf
	Else
		If Empty(cTypeGraf)
			cTypeGraf:=""
		EndIf
		nRET := WAITRUN(cPARGRAF+"grafing.exe "+' '+cTXTGRAF+' '+cTypeGraf)
	EndIf

Return .T.

